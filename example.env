# PostgreSQL connection string
POSTGRES_DSN=    # e.g. host=127.0.0.1 user=postgres password=yourpassword dbname=yourdb port=5432 sslmode=disable

# MinIO configuration
MINIO_ENDPOINT=    # e.g. 127.0.0.1:9000
MINIO_ACCESS_KEY=    # your MinIO access key
MINIO_SECRET_KEY=    # your MinIO secret key
MINIO_USE_SSL=    # true or false
MINIO_BUCKET=    # bucket name to use

# Environment type: use DEV for development, PRO for production
ENV=    # DEV or PRO

# RabbitMQ or other service key
RABBIT_KEY=    # your secret API key or token
